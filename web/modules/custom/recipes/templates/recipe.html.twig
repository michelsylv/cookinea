{#
/**
 * @file recipe.html.twig
 * Default theme implementation to present Recipe entity data.
 *
 * This template is used when viewing Recipe entity pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_recipe()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('recipe') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
